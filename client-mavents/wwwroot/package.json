{
  "name": "nessembly-client",
  "version": "0.0.0",
  "description": "Client Side of Ticket Express",
  "main": "PivotTableUI.js",
  "files": [
    "PivotTable.js",
    "PivotTableUI.js",
    "PlotlyRenderers.js",
    "TableRenderers.js",
